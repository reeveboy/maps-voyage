generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model student {
  id            Int     @id @default(autoincrement())
  first_name    String?
  last_name     String?
  age           Int?
  student_email String
  class         String?
}
